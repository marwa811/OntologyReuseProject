<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY PW "http://purl.org/obo/owl/PW#" >
    <!ENTITY time "http://www.w3.org/2006/time#" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY ro "http://obofoundry.org/ro/ro.owl#" >
    <!ENTITY sequence "http://purl.org/obo/owl/sequence#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY provenir "http://knoesis.wright.edu/provenir/provenir.owl#" >
    <!ENTITY ParasiteLifecycle "http://paige.ctegd.uga.edu/ParasiteLifecycle.owl#" >
    <!ENTITY Thesaurus "http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#" >
]>


<rdf:RDF xmlns="http://knoesis.wright.edu/ParasiteExperiment.owl#"
     xml:base="http://knoesis.wright.edu/ParasiteExperiment.owl"
     xmlns:ro="http://obofoundry.org/ro/ro.owl#"
     xmlns:provenir="http://knoesis.wright.edu/provenir/provenir.owl#"
     xmlns:Thesaurus="http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:time="http://www.w3.org/2006/time#"
     xmlns:PW="http://purl.org/obo/owl/PW#"
     xmlns:sequence="http://purl.org/obo/owl/sequence#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:ParasiteLifecycle="http://paige.ctegd.uga.edu/ParasiteLifecycle.owl#">
    <owl:Ontology rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl">
        <owl:imports rdf:resource="http://knoesis.wright.edu/provenir/provenir.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&rdfs;label"/>
    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_GO_annotation -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_GO_annotation">
        <rdfs:range rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#GO_term"/>
        <rdfs:domain rdf:resource="&sequence;gene"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_input_value -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_input_value">
        <rdfs:comment rdf:datatype="&xsd;string">This property links a process to all its input value. The input values belong to only data_collection (parameter values are linked to process by property has_parameter).</rdfs:comment>
        <rdfs:domain rdf:resource="&provenir;process"/>
        <rdfs:subPropertyOf rdf:resource="&ro;has_participant"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&provenir;data_collection"/>
                    <rdf:Description rdf:about="&provenir;parameter"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_output_value -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_output_value">
        <rdfs:comment rdf:datatype="&xsd;string">This property links a process to all its output value. The output values belong to only data_collection (parameter values are linked to process by property has_parameter).</rdfs:comment>
        <rdfs:domain rdf:resource="&provenir;process"/>
        <rdfs:subPropertyOf rdf:resource="&ro;has_participant"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&provenir;data_collection"/>
                    <rdf:Description rdf:about="&provenir;parameter"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_primer -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_primer">
        <rdfs:range rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#primer"/>
        <rdfs:subPropertyOf rdf:resource="&ro;part_of"/>
        <rdfs:domain rdf:resource="&sequence;gene"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_region -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_region">
        <rdfs:domain rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#primer"/>
        <rdfs:subPropertyOf rdf:resource="&ro;part_of"/>
        <rdfs:range rdf:resource="&sequence;region"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_status_value -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_status_value">
        <rdfs:range rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#status"/>
        <rdfs:subPropertyOf rdf:resource="&provenir;has_parameter"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#involved_in -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#involved_in">
        <rdfs:range rdf:resource="&provenir;data_collection"/>
        <rdfs:domain rdf:resource="&provenir;parameter"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#is_allele_of -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#is_allele_of">
        <rdfs:comment rdf:datatype="&xsd;string">If two paralogous genes occur in the same gene, they are alleles</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#is_paralogous_to"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#is_homologous_to -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#is_homologous_to">
        <rdfs:subPropertyOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#is_similar_to"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#is_identical_to -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#is_identical_to">
        <rdfs:subPropertyOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#is_similar_to"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#is_orthologous_to -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#is_orthologous_to">
        <rdfs:subPropertyOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#is_similar_to"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#is_paralogous_to -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#is_paralogous_to">
        <rdfs:subPropertyOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#is_similar_to"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#is_similar_to -->

    <owl:ObjectProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#is_similar_to">
        <rdfs:comment rdf:datatype="&xsd;string"></rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&ro;transformation_of"/>
        <rdfs:domain rdf:resource="&sequence;region"/>
        <rdfs:range rdf:resource="&sequence;region"/>
    </owl:ObjectProperty>
    


    <!-- http://knoesis.wright.edu/provenir/provenir.owl#has_parameter -->

    <owl:ObjectProperty rdf:about="&provenir;has_parameter"/>
    


    <!-- http://obofoundry.org/ro/ro.owl#has_agent -->

    <owl:ObjectProperty rdf:about="&ro;has_agent"/>
    


    <!-- http://obofoundry.org/ro/ro.owl#has_participant -->

    <owl:ObjectProperty rdf:about="&ro;has_participant"/>
    


    <!-- http://obofoundry.org/ro/ro.owl#part_of -->

    <owl:ObjectProperty rdf:about="&ro;part_of"/>
    


    <!-- http://obofoundry.org/ro/ro.owl#preceded_by -->

    <owl:ObjectProperty rdf:about="&ro;preceded_by"/>
    


    <!-- http://obofoundry.org/ro/ro.owl#transformation_of -->

    <owl:ObjectProperty rdf:about="&ro;transformation_of"/>
    


    <!-- http://paige.ctegd.uga.edu/ParasiteLifecycle.owl#has_base_strain -->

    <owl:ObjectProperty rdf:about="&ParasiteLifecycle;has_base_strain">
        <rdfs:domain rdf:resource="&ParasiteLifecycle;organism_strain"/>
        <rdfs:range rdf:resource="&ParasiteLifecycle;organism_strain"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->

    <owl:ObjectProperty rdf:about="&owl;topObjectProperty"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_amount -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_amount">
        <rdfs:range rdf:resource="&xsd;float"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_genes_in_locus -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_genes_in_locus">
        <rdfs:subPropertyOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#has_amount"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_hmm_prob -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_hmm_prob">
        <rdfs:domain rdf:resource="&provenir;domain_parameter"/>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_nn_d -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_nn_d">
        <rdfs:domain rdf:resource="&provenir;domain_parameter"/>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_nn_sum -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_nn_sum">
        <rdfs:domain rdf:resource="&provenir;domain_parameter"/>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_plasmid_completed -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_plasmid_completed">
        <rdfs:subPropertyOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#has_amount"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_plasmid_in_progress -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_plasmid_in_progress">
        <rdfs:subPropertyOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#has_amount"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_plasmid_needed -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_plasmid_needed">
        <rdfs:subPropertyOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#has_amount"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_tm_count -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_tm_count">
        <rdfs:domain rdf:resource="&provenir;domain_parameter"/>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_transfection_attempts -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_transfection_attempts">
        <rdfs:subPropertyOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#has_amount"/>
        <rdfs:domain rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#strain_creation_protocol"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#has_value -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#has_value">
        <rdfs:domain rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#peptide_count"/>
        <rdfs:domain rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#statistical_measure"/>
        <rdfs:range rdf:resource="&xsd;double"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#insertion_confirmed -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#insertion_confirmed">
        <rdfs:range rdf:resource="&xsd;boolean"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#is_episomal -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#is_episomal">
        <rdfs:range rdf:resource="&xsd;boolean"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#is_null_mutant -->

    <owl:DatatypeProperty rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#is_null_mutant">
        <rdfs:domain rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#strain_creation_protocol"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topDataProperty -->

    <owl:DatatypeProperty rdf:about="&owl;topDataProperty"/>
    


    <!-- http://www.w3.org/2006/time#day -->

    <owl:DatatypeProperty rdf:about="&time;day">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;date"/>
        <rdfs:domain rdf:resource="&time;DateTimeDescription"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#hour -->

    <owl:DatatypeProperty rdf:about="&time;hour">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;int"/>
        <rdfs:domain rdf:resource="&time;DateTimeDescription"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#minute -->

    <owl:DatatypeProperty rdf:about="&time;minute">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;int"/>
        <rdfs:domain rdf:resource="&time;DateTimeDescription"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2006/time#second -->

    <owl:DatatypeProperty rdf:about="&time;second">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="&xsd;float"/>
        <rdfs:domain rdf:resource="&time;DateTimeDescription"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#COG_info -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#COG_info">
        <rdfs:subClassOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#researcher_note"/>
        <rdfs:comment>The set of genes in Leishmania major and T. brucei, if any, that are orthologous to a given T. cruzi gene.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#DBchangelog -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#DBchangelog">
        <rdfs:subClassOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#technician_note"/>
        <rdfs:comment>Automatically produced log of any changes that are made in the database, including the date they were made and the name of the person who made them.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#DateCompleted -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#DateCompleted">
        <rdfs:subClassOf rdf:resource="&time;DateTimeDescription"/>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#DateOfLastModification -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#DateOfLastModification">
        <rdfs:subClassOf rdf:resource="&time;DateTimeDescription"/>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#DateStarted -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#DateStarted">
        <rdfs:subClassOf rdf:resource="&time;DateTimeDescription"/>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#GO_term -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#GO_term">
        <rdfs:subClassOf rdf:resource="&provenir;data_collection"/>
        <rdfs:comment>A node in an ontological tree.  Can refer to biological process, cellular component, or molecular function.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#KOlog -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#KOlog">
        <rdfs:subClassOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#experiment_justification"/>
        <rdfs:comment>Log of what has been done to date in the production of KO plasmids for a given KO target.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#SAM_result -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#SAM_result">
        <rdfs:subClassOf rdf:resource="&provenir;data_collection"/>
        <rdfs:comment>The significance, or not, of the degree of gene expression regulation for a gene as determined from microarray analysis using SAM analysis, which is a statistical test for significance.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#Tcruzi_lifecyclestage_subsample -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#Tcruzi_lifecyclestage_subsample">
        <rdfs:subClassOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#sample"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ro;part_of"/>
                <owl:allValuesFrom rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#Tcruzi_sample"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Growth of specific lifecycle stage of Trypanosoma cruzi.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#Tcruzi_sample -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#Tcruzi_sample">
        <rdfs:subClassOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#sample"/>
        <rdfs:comment rdf:datatype="&xsd;string">Trypanosoma cruzi cells in growth medium</rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#accession_number -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#accession_number">
        <rdfs:subClassOf rdf:resource="&provenir;domain_parameter"/>
        <rdfs:comment rdf:datatype="&xsd;string">An unique identifier given to a DNA or protein sequence record.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#allele_designation -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#allele_designation">
        <rdfs:subClassOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#laboratory_experiment_parameters"/>
        <rdfs:comment>For diploid (or polyploid) organisms where a knockout construct plasmid is created for each allele of a gene, the allele_designation is the assignment of allele number for a particular knockout plasmid.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#allele_ko_log -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#allele_ko_log">
        <rdfs:subClassOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#researcher_note"/>
        <rdfs:comment>Log of what has been done to date in the production of a KO plasmid for a given KO target for a specific allele of that target gene. </rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#assembly_confidence_notes -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#assembly_confidence_notes">
        <rdfs:subClassOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#experiment_notes"/>
        <rdfs:comment>Description of the likelihood that the published and assembled sequence for a given locus in the genome is correct.</rdfs:comment>
    </owl:Class>
    


    <!-- http://knoesis.wright.edu/ParasiteExperiment.owl#cell_cloning -->

    <owl:Class rdf:about="http://knoesis.wright.edu/ParasiteExperiment.owl#cell_cloning">
        <rdfs:subClassOf rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#strain_creation_process"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&ro;part_of"/>
                <owl:someValuesFrom rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#strain_creation_protocol"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#has_input_value"/>
                <owl:allValuesFrom rdf:resource="http://knoesis.wright.edu/ParasiteExperiment.owl#drug_selected_sample"/>
            </owl:Restriction>
