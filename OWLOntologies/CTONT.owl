    xmlns:dc="http://purl.org/dc/elements/1.1/"
  xml:base="http://epoch.stanford.edu/ClinicalTrialOntology.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://protege.stanford.edu/plugins/owl/protege"/>
    <owl:imports rdf:resource="http://epoch.stanford.edu/ProtocolOntology.owl"/>
    <owl:imports rdf:resource="http://epoch.stanford.edu/OrganizationOntology.owl"/>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;table border="1">
      &lt;tr>
        &lt;td>
          &lt;b>&lt;i>Version&lt;/i>&lt;/b>
        &lt;/td>
        &lt;td>
          0.97
        &lt;/td>
      &lt;/tr>
      &lt;tr>
        &lt;td>
          &lt;b>&lt;i>Date&lt;/i>&lt;/b>
        &lt;/td>
        &lt;td>
          3/2/2007&lt;br>
        &lt;/td>
      &lt;/tr>
    &lt;/table>
    &lt;br></owl:versionInfo>
    <protege:defaultLanguage rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >en</protege:defaultLanguage>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
