    <!-- http://cui.unige.ch/isi/onto/tok/TOK.owl#has_align -->

    <owl:ObjectProperty rdf:about="#has_align">
        <rdfs:comment rdf:datatype="&xsd;string"
            >this iproperty is a link between a TOK resource and its alignment.</rdfs:comment>
        <ace_lexicon:sg>has_aligns</ace_lexicon:sg>
        <ace_lexicon:vbg>has_aligned</ace_lexicon:vbg>
        <ace_lexicon:pl>has_align</ace_lexicon:pl>
    </owl:ObjectProperty>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;Axiom"/>
        <acetext:acetext
            >If X has_aligns Y then Y has_align_targets X. If X has_align_targets Y then Y has_aligns X.</acetext:acetext>
        <owl:object rdf:resource="#has_align"/>
        <owl:subject rdf:resource="#has_align_target"/>
        <owl:predicate rdf:resource="&owl;inverseOf"/>
    </rdf:Description>
    <rdf:Description>
        <rdf:type rdf:resource="&owl;Axiom"/>
        <acetext:acetext
            >If X has_aligns Y then Y has_align_sources X. If X has_align_sources Y then Y has_aligns X.</acetext:acetext>
        <owl:object rdf:resource="#has_align"/>
        <owl:subject rdf:resource="#has_align_source"/>
        <owl:predicate rdf:resource="&owl;inverseOf"/>
    </rdf:Description>
    


