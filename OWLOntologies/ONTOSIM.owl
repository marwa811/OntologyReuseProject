# Object Property: SIM:caracterizaIdentificacao (Characterize identification)

AnnotationAssertion(rdfs:label SIM:caracterizaIdentificacao "Caracteriza Identificação"@pt)
AnnotationAssertion(rdfs:label SIM:caracterizaIdentificacao "Characterize identification"@en)
SubObjectPropertyOf(SIM:caracterizaIdentificacao owl:topObjectProperty)
InverseObjectProperties(SIM:caracterizaIdentificacao SIM:eCaracterizadaPorCausas)
ObjectPropertyDomain(SIM:caracterizaIdentificacao SIM:CausasDoObito)
ObjectPropertyRange(SIM:caracterizaIdentificacao SIM:Identificacao)

