        <englishTitle>Hemeralopia</englishTitle>
        <persianTitle>خفش</persianTitle>
    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#kharesh_pestan -->

