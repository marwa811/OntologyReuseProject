        <rdfs:domain rdf:resource="http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#Presential"/>
        <rdfs:range rdf:resource="http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#Time_boundary"/>
        <dc:description>Presentials exist at a single time boundary.</dc:description>
        <rdfs:label xml:lang="en">exists at</rdfs:label>
    </owl:ObjectProperty>
    


