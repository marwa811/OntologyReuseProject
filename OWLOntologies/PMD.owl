        <englishTitle>Nasal dryness</englishTitle>
        <persianTitle>جفاف الأنف‏</persianTitle>
    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#jahr -->

