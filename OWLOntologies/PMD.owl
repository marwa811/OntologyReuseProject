        <englishTitle></englishTitle>
        <persianTitle>كثرة درور المنى و المذى و الوذى‏</persianTitle>
    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#kesrat_shahvat -->

