        <englishTitle>polymenorrhagia</englishTitle>
        <persianTitle>كثرة الطمث‏</persianTitle>
    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#kesrate_laban -->

