        <englishTitle></englishTitle>
        <persianTitle>انتشار الشعر و الصلع‏</persianTitle>
    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#isterkha -->

