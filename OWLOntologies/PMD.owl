        <englishTitle>Paralysis</englishTitle>
        <englishTitle>Paresis</englishTitle>
        <englishTitle>Relaxation</englishTitle>
        <persianTitle>إسترخاء</persianTitle>
    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#isterkha_jafan -->

