        <englishTitle>Lens disorders</englishTitle>
        <englishTitle>diseases of lens</englishTitle>
        <persianTitle>علل الرطوبة الجلیدیه</persianTitle>
    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#jarab_ajfan -->

