        <diseaseDescription>سستى عضلات و عصب مقعد</diseaseDescription>
        <englishTitle>Anal atony and muscle paralysis</englishTitle>
        <persianTitle>استرخاء الشرج‏</persianTitle>
    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#itesal_jafan -->

