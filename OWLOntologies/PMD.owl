        <englishTitle>Pruritic lesion of the eyelids</englishTitle>
        <persianTitle>جرب الأجفان‏</persianTitle>
    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#jasa -->

