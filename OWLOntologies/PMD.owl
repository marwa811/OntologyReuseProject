        <persianTitle>اتصاق الجفنين‏</persianTitle>
    </owl:Class>
    


    <!-- http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#jafaf_anf -->

    <owl:Class rdf:about="http://www.onto-med.de/ontologies/gfo-persian-medicine-diseases.owl#jafaf_anf">
