     xml:base="http://neurolog.unice.fr/ontoneurolog/v3.0/ontoneurolog-dataset-processing.owl"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:dataset="http://neurolog.unice.fr/ontoneurolog/v3.0/dataset.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:dolce-particular="http://neurolog.unice.fr/ontoneurolog/v3.0/dolce-particular.owl#"
     xmlns:medical-dataset-expression="http://neurolog.unice.fr/ontoneurolog/v3.0/medical-dataset-expression.owl#"
     xmlns:core-ontology-programs-software="http://neurolog.unice.fr/ontoneurolog/v3.0/core-ontology-programs-software.owl#"
