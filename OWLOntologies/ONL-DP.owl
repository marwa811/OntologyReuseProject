        <dc:contributor rdf:datatype="&rdfs;Literal">Farooq Ahmad, Michel Dojat, Bernard Gibaud, Gilles Kassel, Lynda Temal, bernard.gibaud@univ-rennes1.fr</dc:contributor>
        <dc:date>2009-11-20</dc:date>
        <owl:imports rdf:resource="http://neurolog.unice.fr/ontoneurolog/v3.0/dataset.owl"/>
    </owl:Ontology>
    


