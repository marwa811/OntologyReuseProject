    <!-- http://neurolog.unice.fr/ontoneurolog/v3.0/ontoneurolog-dataset-processing.owl#has-for-floating-dataset-at -->

    <owl:ObjectProperty rdf:about="#has-for-floating-dataset-at">
        <rdfs:domain rdf:resource="#registration"/>
        <rdfs:range rdf:resource="&dataset;floating-dataset"/>
        <rdfs:subPropertyOf rdf:resource="&ontoneurolog-extension-of-dolce;has-for-data-at"/>
    </owl:ObjectProperty>
    


    <!-- http://neurolog.unice.fr/ontoneurolog/v3.0/ontoneurolog-dataset-processing.owl#has-for-reference-dataset-at -->

    <owl:ObjectProperty rdf:about="#has-for-reference-dataset-at">
        <rdfs:domain rdf:resource="#registration"/>
        <rdfs:range rdf:resource="&dataset;reference-dataset"/>
        <rdfs:subPropertyOf rdf:resource="&ontoneurolog-extension-of-dolce;has-for-data-at"/>
    </owl:ObjectProperty>
    


    <!-- http://neurolog.unice.fr/ontoneurolog/v3.0/dolce-particular.owl#has-for-part -->

    <owl:ObjectProperty rdf:about="&dolce-particular;has-for-part"/>
    


    <!-- http://neurolog.unice.fr/ontoneurolog/v3.0/ontoneurolog-extension-of-dolce.owl#has-for-data-at -->

    <owl:ObjectProperty rdf:about="&ontoneurolog-extension-of-dolce;has-for-data-at"/>
    


    <!-- http://neurolog.unice.fr/ontoneurolog/v3.0/ontoneurolog-extension-of-dolce.owl#has-for-result-at -->

    <owl:ObjectProperty rdf:about="&ontoneurolog-extension-of-dolce;has-for-result-at"/>
    


    <!-- http://neurolog.unice.fr/ontoneurolog/v3.0/ontoneurolog-extension-of-dolce.owl#is-a-result-of-at -->

