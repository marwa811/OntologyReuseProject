<http://rdf.biosemantics.org/ontologies/rsa> rdf:type owl:Ontology ;
                                              dc:contributor "Andrew P. Gibson"^^xsd:string ,
                                                             "Jeroen F.J. Laros"^^xsd:string ;
                                              dc:created "2012-10-08T00:00:00"^^xsd:dateTime ;
