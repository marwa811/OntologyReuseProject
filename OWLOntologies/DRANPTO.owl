        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/FamilyMemberFocusedIntervention"/>
        <rdfs:comment xml:lang="en">It refers to the training on family member&apos;s nonverbal interaction techniques for communicating with people living with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Nonverbal Communication Skill Education</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OccupationalActivity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OccupationalActivity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/StructuredActivity"/>
        <ncit:abbreviation xml:lang="en">OA</ncit:abbreviation>
        <rdfs:comment xml:lang="en">It refers to the particular organized activities relating to a job or profession of a person.</rdfs:comment>
        <rdfs:label xml:lang="en">Occupational Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OfferingChoice -->

