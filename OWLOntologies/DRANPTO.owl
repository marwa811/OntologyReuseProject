    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RepetitiveQuestioning -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RepetitiveQuestioning">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/VerballyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Questioning repetitively</rdfs:comment>
        <rdfs:label xml:lang="en">Repetitive Questioning</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Repositioning -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Repositioning">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/GeneralNursingCareIntervention"/>
        <rdfs:comment xml:lang="en">It refers to the change of the position of person with dementia, which might be not comfortable.</rdfs:comment>
        <rdfs:label xml:lang="en">Repositioning</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ReschedulingRoutine -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ReschedulingRoutine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/GeneralNursingCareIntervention"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/reducesAgitationCausedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriateRoutine"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the rescheduling the care routine of person with dementia according to his/her needs.</rdfs:comment>
        <rdfs:label xml:lang="en">Rescheduling Routine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ResidentBehaviour -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ResidentBehaviour">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InterpersonalCausativeFactor"/>
        <ncit:synonym xml:lang="en">Behaviour of Other Person with Dementia</ncit:synonym>
        <rdfs:comment xml:lang="en">The agitated behaviour of one resident can bring about agitated behaviour in other residents. It&apos;s also called epidemic agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Epidemic Agitation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Resisting -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Resisting">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyAggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">It refers to the refusal to accept or comply with something.</rdfs:comment>
        <rdfs:label xml:lang="en">Resisting</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ResolutionTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ResolutionTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualTherapyForAgitation"/>
        <rdfs:comment xml:lang="en">Resolution Therapy is a one-to-one therapeutic approach to be conducted with sensitivity, patience and tolerance. It is not practised at certain times, in sessions or groups, but characterises the everyday interaction between all who come into contact with those with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Resolution Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RespiteActivity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RespiteActivity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SupportInterventionToHealthCareProfessional"/>
        <rdfs:comment xml:lang="en">‘Respite’ or ‘respite care’ is when someone else takes care of the person the health care professional supposed care for, so that the health care professional can have a break. A break can give the health care professional time to do everyday activities or just to relax, deal with stress and look after herself/himself.</rdfs:comment>
        <rdfs:label xml:lang="en">Respite Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Restlessness -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Restlessness">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">The inability to rest or relax</rdfs:comment>
        <rdfs:label xml:lang="en">Restlessness</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Restriction -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Restriction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BehaviourTherapy"/>
        <rdfs:comment xml:lang="en">Restriction: denying the person goods, activities, etc when agitated</rdfs:comment>
        <rdfs:label xml:lang="en">Restriction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Roaring -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Roaring">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/VerballyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Making a very loud, deep, prolonged sound</rdfs:comment>
        <rdfs:label xml:lang="en">Roaring</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RoboticAnimalAssistedTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RoboticAnimalAssistedTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AnimalAssistedTherapy"/>
        <rdfs:comment xml:lang="en">It refers to a psychotherapy treatment based upon building a bond with a robot animal through interaction and play. For example, the therapeutic pet-type robot, PARO, was developed by Japanese engineers as an alternative to animal-assisted therapy. Modeled on the feature of a baby harp seal and weighing approximately 2.5 kg, RARO has 5 in-built sensors that enable the robot to respond autonomously to the user and his or her environment. PARO is diurnally active, and can move its flippers and tail, open and close its eyes, and make sounds similar to a baby harp seal.</rdfs:comment>
        <rdfs:label xml:lang="en">Robotic Animal-Assisted Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Rocking -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Rocking">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Behaviour of moving gently to and from side to side</rdfs:comment>
        <rdfs:label xml:lang="en">Rocking</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RoleIdentity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RoleIdentity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Characteristic"/>
        <rdfs:comment xml:lang="en">It refers to the role (or character) people play when holding specific social positions in groups.</rdfs:comment>
        <rdfs:label xml:lang="en">Role Identity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RoomTemperature -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RoomTemperature">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Temperature"/>
        <rdfs:comment xml:lang="en">Improperly heated areas or cold areas can cause agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Room Temperature</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Rummaging -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Rummaging">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Rummaging, that is, disorganised foraging</rdfs:comment>
        <rdfs:label xml:lang="en">Rummaging</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/STARVABehaviouralApproach -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/STARVABehaviouralApproach">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementApproach"/>
        <rdfs:comment xml:lang="en">STAR-VA, an interdisciplinary behavioural approach for managing challenging dementia-related behaviours in the nursing home care settings. Briefly, STAR-VA has three core components – (1) ABCs: identifying and changing intrapersonal, interpersonal, or environmental Activators to and Consequences that may reinforce or exacerbate the challenging Behaviours; (2) Pleasant Events: identifying and increasing, through a structured and individualized process, personally relevant and meaningful pleasant events consistent with the individual’s abilities; and (3) Realistic Expectations and Communication Skills: Promoting realistic expectations of residents with dementia and increasing effective verbal and nonverbal communication strategies with these residents, including the Listen with Respect, Comfort, and Redirect approach.</rdfs:comment>
        <rdfs:label xml:lang="en">STAR-VA Behaviour Approach</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SadnessOfHealthCareProfessional -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SadnessOfHealthCareProfessional">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EffectOnMentalHealthOfHealthCareProfessional"/>
        <rdfs:comment xml:lang="en">It refers to the concept that the agitated behaviour of people with dementia can cause sadness of health care professionals.</rdfs:comment>
        <rdfs:label xml:lang="en">Sadness Of Health Care Professional</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ScaleForTheObservationOfAgitationInPersonsWithDementia -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ScaleForTheObservationOfAgitationInPersonsWithDementia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationAssessmentTool"/>
        <ncit:abbreviation xml:lang="en">SOAPD</ncit:abbreviation>
        <rdfs:comment xml:lang="en">It is a seven-item scale that rated each behaviour on duration and, for two behaviours, on intensity. The categories were: 1) total body movements from once place, 2) up/down movements, 3) repetitive body motions in one place, 4) outward motions, 5) high pitched/loud noise, 6) repetitive vocalization, and 7) negative words.</rdfs:comment>
        <rdfs:label xml:lang="en">Scale for the Observation of Agitation in Persons with Dementia</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Scratching -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Scratching">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyAggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Behaviour of marking the surface of something with a sharp or pointed object</rdfs:comment>
        <rdfs:label xml:lang="en">Scratching</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Screaming -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Screaming">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/VerballyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Making a loud, piercing sound</rdfs:comment>
        <rdfs:label xml:lang="en">Screaming</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ScrewingNut -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ScrewingNut">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Ergotherapy"/>
        <rdfs:comment xml:lang="en">It refers to the organized activity of screwing nuts.</rdfs:comment>
        <rdfs:label xml:lang="en">Screwing Nut</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Searching -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Searching">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Behaviour of thoroughly scrutinizing, especially in a disconcering way</rdfs:comment>
        <rdfs:label xml:lang="en">Searching</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SelectingCostumeJewellery -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SelectingCostumeJewellery">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RecreationalActivity"/>
        <rdfs:comment xml:lang="en">It refers to the activity of selecting costume jewellery.</rdfs:comment>
        <rdfs:label xml:lang="en">Selecting Costume Jewelley</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryAbility -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryAbility">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RemainingAbility"/>
        <ncit:synonym xml:lang="en">Sensory Acuity, Sensory Perception</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the ability of the sense organs (e.g., eye, ear, nose, skin) to receive input accurately from the sense organs.</rdfs:comment>
        <rdfs:label xml:lang="en">Sensory Ability</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryImpairment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryImpairment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Deficiency"/>
        <rdfs:comment xml:lang="en">It refers to the damaged physical senses.</rdfs:comment>
        <rdfs:label xml:lang="en">Sensory Impairment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualTherapyForAgitation"/>
        <ncit:synonym xml:lang="en">Sensory Stimulation Therapy</ncit:synonym>
        <rdfs:comment xml:lang="en">Sensory therapy uses stimulation of the different senses such as touch, smell, hearing and sight to promote wellbeing and reduce agitation in people with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Sensory Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SevereImpairmentBattery -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SevereImpairmentBattery">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationAssessmentTool"/>
        <ncit:abbreviation xml:lang="en">SIB</ncit:abbreviation>
        <rdfs:comment xml:lang="en">The Severe Impairment Battery (SIB) was developed to assess the skills of people with severe dementia who are too impaired to complete standard neuropsychological tests.The commercial version consists of 40 simple one-step commands and gestural cues, presented in a relatively naturalistic, conversational style. The SIB requires simple responses from the examinee, with points being awarded for approximations and partially correct responses as well as for fully correct ones, and prompts being included in the event of “don’t know” or incorrect responses. The maximum score is 100.</rdfs:comment>
        <rdfs:label xml:lang="en">Severe Impairment Battery</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SeverityOfAgitation -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SeverityOfAgitation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/FeatureOfAgitation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isSeverityOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationInDementia"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the condition of being severe.</rdfs:comment>
        <rdfs:label xml:lang="en">Severity Of Agitation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Sewing -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Sewing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Ergotherapy"/>
        <rdfs:comment xml:lang="en">It refers to the organized activity of sewing.</rdfs:comment>
        <rdfs:label xml:lang="en">Sewing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Shouting -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Shouting">
