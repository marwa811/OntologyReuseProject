        <rdfs:label xml:lang="en">Speaking In Unhurried Voice</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SpeakingLocalLanguageAsSecondLanguage -->

