        <rdfs:comment xml:lang="en">Training of helping health care professionals to manage the triggers that cause agitated behaviours in people with dementia</rdfs:comment>
        <rdfs:label xml:lang="en">Trigger Managing Training</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TriggerRecognisingTraining -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TriggerRecognisingTraining">
