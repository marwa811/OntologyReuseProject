        <ncit:synonym xml:lang="en">Psychotic Disorder</ncit:synonym>
        <rdfs:comment xml:lang="en">A mental disorder characterised by a disconnection from reality.</rdfs:comment>
        <rdfs:label xml:lang="en">psychotic symptom</rdfs:label>
    </owl:Class>
    


