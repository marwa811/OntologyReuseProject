        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InternalEnvironmentModification"/>
        <rdfs:comment xml:lang="en">It refers to the progressive decrease of the illuminance at night (6:00 – 8:00 pm).</rdfs:comment>
        <rdfs:label xml:lang="en">Progressive Decrease Illuminance at Night</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ProgressiveMuscleRelaxation -->

