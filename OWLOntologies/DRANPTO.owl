        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationInDementia"/>
        <rdfs:comment xml:lang="en">Behaviours that are not intending to cause physical harm, but departing from an accepted standard</rdfs:comment>
        <rdfs:label xml:lang="en">Physically Nonaggressive Behaviour</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PittsburghAgitationScale -->

