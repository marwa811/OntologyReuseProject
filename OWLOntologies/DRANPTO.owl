        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Light"/>
        <rdfs:comment xml:lang="en">Poorly lighting can cause agitation in dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Poorly Lighting</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PositivePracticeStyleOfHealthCareProfessional -->

