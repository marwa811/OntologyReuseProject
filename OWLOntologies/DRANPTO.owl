        <rdfs:comment xml:lang="en">It refers to the acitivity requring physical effort for sustaining or improving health and fitness.</rdfs:comment>
        <rdfs:label xml:lang="en">Exercise</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ExternalEnvironmentModification -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ExternalEnvironmentModification">
