        <rdfs:comment xml:lang="en">Training to help health care professionals to identify the triggers of agitated behaviours in people with dementia</rdfs:comment>
        <rdfs:label xml:lang="en">Trigger Recognising Training</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/UnderStimulationOfSocialEnvironment -->

