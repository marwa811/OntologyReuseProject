        <rdfs:comment xml:lang="en">The action of manipulating something constantly</rdfs:comment>
        <rdfs:label xml:lang="en">Constant Manipulation Of Object</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ConstantRepetitionOfWord -->

