        <ncit:abbreviation xml:lang="en">DV</ncit:abbreviation>
        <rdfs:comment xml:lang="en">It refers to the vocally disruptive behaviour.</rdfs:comment>
        <rdfs:label xml:lang="en">Disruptive Vocalisation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Distraction -->

