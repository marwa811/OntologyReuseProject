                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Repositioning"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Position that is not comfortable for person with dementia. For example, sitting all day in an uncomfortable position</rdfs:comment>
        <rdfs:label xml:lang="en">Discomfortable Position</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/DisruptionToCareRoutine -->

