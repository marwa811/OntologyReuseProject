        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/VerballyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">It refers to the constant repetition of words or sentences.</rdfs:comment>
        <rdfs:label xml:lang="en">Constant Repetition of Word</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ConstantUnwarrantedRequestsForAttention -->

