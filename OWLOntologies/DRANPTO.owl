    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TraumaticBrainInjury -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TraumaticBrainInjury">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrapersonalCausativeFactor"/>
        <ncit:synonym xml:lang="en">Craniocerebral Trauma</ncit:synonym>
        <rdfs:comment xml:lang="en">Brain dysfunction caused by an outside force, usually a violent blow to the head.</rdfs:comment>
        <rdfs:label xml:lang="en">Traumatic Brain Injury</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TreatmentPlanCreation -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TreatmentPlanCreation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementActivity"/>
        <rdfs:comment xml:lang="en">It refers to the activity of creating the treatment plan for a person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Treatment Plan Creation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TreatmentRoutesForExploringAgitation -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TreatmentRoutesForExploringAgitation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementApproach"/>
        <ncit:abbreviation xml:lang="en">TREA</ncit:abbreviation>
        <rdfs:comment xml:lang="en">TREA is an objective, systematic method for developing individualized non-pharmacological treatment plans based on an analysis of the agitated person&apos;s unmet needs, past and current preferences, past role-identity, cognitive, mobility, and sensory abilities/limitations, and possible causes for particular agitated behaviors.</rdfs:comment>
        <rdfs:label xml:lang="en">Treatment Routes for Exploring Agitation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TriggerManagingTraining -->

