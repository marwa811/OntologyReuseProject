        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PharmacologicalIntervention"/>
        <ncit:synonym xml:lang="en">environmental and psychosocial intervention</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the intervention which does not use drugs.</rdfs:comment>
        <rdfs:label xml:lang="en">Nonpharmacological Intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NonverbalCommunicationSkillEducation -->

