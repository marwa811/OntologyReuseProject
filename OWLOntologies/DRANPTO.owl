        <rdfs:label xml:lang="en">Environmental Modification Training</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EnvironmentalVulnerabilityModel -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EnvironmentalVulnerabilityModel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementApproach"/>
        <ncit:synonym xml:lang="en">Reduced Stress Threshold Model</ncit:synonym>
        <rdfs:comment xml:lang="en">Environmental vulnerability model assumes that the dementa process results in greater vulnerability to the environment and a lower threshold at which stimuli affect behaviour. Therefore, the effective treatment is to identify and eliminate stimuli that trigger problem behaviour.</rdfs:comment>
        <rdfs:label xml:lang="en">Environmental Vulnerability Model</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Ergotherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Ergotherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/StructuredActivity"/>
        <rdfs:comment xml:lang="en">Ergotherapy involves of practicing of retrained capabilities that are relevant to everyday life and of emotionally positive activities (e.g., cooking, gardening), with reference to the patient’s life history</rdfs:comment>
        <rdfs:label xml:lang="en">Ergotherapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EthnicityRoleIdentity -->

