        <rdfs:comment xml:lang="en">It refers to things people do fun and enjoyment.</rdfs:comment>
        <rdfs:label xml:lang="en">Recreational Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RecreationalTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RecreationalTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualTherapyForAgitation"/>
        <ncit:abbreviation xml:lang="en">RT</ncit:abbreviation>
        <rdfs:label xml:lang="en">Recreational Therapy</rdfs:label>
        <skos:definition xml:lang="en">Recreational therapy is a systematic process that utilizes recreation and other activity-based interventions to address the assessed needs of individuals with illness and/or disabling condition, as a means to psychological and physical health, recovery and well-being. Defined by National Council for Therapeutic Recreation Certification https://www.nctrc.org/about-ncrtc/about-recreational-therapy/</skos:definition>
        <skos:definition xml:lang="en">Therapeutic Recreation Therapy, Therapeutic Recreation</skos:definition>
    </owl:Class>
    


