        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/GeneralNursingCareIntervention"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/reducesAgitationCausedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Thirst"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the intervention of offering drink to person with dementia, such as water, warm milk or tea</rdfs:comment>
        <rdfs:label xml:lang="en">Offering Drink</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OfferingOptionOfWashingSelf -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OfferingOptionOfWashingSelf">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/GeneralNursingCareInterventionForAgitationDuringBathing"/>
        <rdfs:comment xml:lang="en">Health care professionals should offer person with dementia the option of washing him/herself.</rdfs:comment>
        <rdfs:label xml:lang="en">Offering Option of Washing Self</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OneOnOneSocialInteraction -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OneOnOneSocialInteraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RealHumanSocialContactIntervention"/>
        <rdfs:comment xml:lang="en">It refers to the treatment that involves of one-on-one social interaction.</rdfs:comment>
        <rdfs:label xml:lang="en">One On One Social Interaction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OneToOneThemedReminiscenceTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OneToOneThemedReminiscenceTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ReminiscenceTherapy"/>
        <rdfs:comment xml:lang="en">It refers to the reminiscence therapy conducted with themes in one to one style.</rdfs:comment>
        <rdfs:label xml:lang="en">One to One Themed Reminiscence Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OrganizationFinancialChallenge -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OrganizationFinancialChallenge">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BarrierToImplementNonpharmacologicalIntervention"/>
        <rdfs:comment xml:lang="en">It refers to the concept that long-term care organizaiton&apos;s financial challenge could be a barrier to implement nonpharmacological intervention to agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Organization Financial Challenge</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OrganizationManagementStructuralConstraint -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OrganizationManagementStructuralConstraint">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BarrierToImplementNonpharmacologicalIntervention"/>
        <rdfs:comment xml:lang="en">It refers to the concept that organization&apos;s management structural constraint could be a barrier to implement nonpharmacological interventions to agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Organization Management Structural Constraint</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OrganizationalCulture -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OrganizationalCulture">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SocialEnvironmentFactor"/>
        <rdfs:comment xml:lang="en">The culture of the long-term care facilities  where person with dementia lives.</rdfs:comment>
        <rdfs:label xml:lang="en">Organizational Culture</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Outburst -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Outburst">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EmotionalDistress"/>
        <rdfs:comment xml:lang="en">A sudden release of strong emotion.</rdfs:comment>
        <rdfs:label xml:lang="en">Outburst</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OutdoorGardening -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OutdoorGardening">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Ergotherapy"/>
        <rdfs:comment xml:lang="en">It refers to the organized activity of gardening in the outdoor environment.</rdfs:comment>
        <rdfs:label xml:lang="en">Outdoor Gardening</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OutdoorWalk -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OutdoorWalk">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicalActivity"/>
        <rdfs:comment xml:lang="en">It refers to the activty of walking in the outdoor environment.</rdfs:comment>
        <rdfs:label xml:lang="en">Outdoor Walk</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OverMedication -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OverMedication">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EffectOfAgitationOnPersonWithDementia"/>
        <rdfs:comment xml:lang="en">It refers to the concept that agitation could cause over medication to person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Over Medication</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OverStimulationOfSocialEnvironment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/OverStimulationOfSocialEnvironment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SocialEnvironmentFactor"/>
        <rdfs:comment xml:lang="en">Over-stimulation from the social environment can cause agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Over Stimulation Of Social Environment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PANSSExcitedComponent -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PANSSExcitedComponent">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationAssessmentTool"/>
        <ncit:abbreviation>PANSS-EC</ncit:abbreviation>
        <ncit:synonym xml:lang="en">Excited Component of the Positive and Negative Syndrome Scale</ncit:synonym>
        <rdfs:comment xml:lang="en">The PANSS Excited Component (PANSS-EC) is  used to assess agitated patients. The PANSS-EC consists of 5 items: excitement, tension, hostility, uncooperativeness, and poor impulse control. The 5 items from the PANSS-EC are rated from 1 (not present) to 7 (extremely severe); scores range from 5 to 35; mean scores ≥ 20 clinically correspond to severe agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">PANSSE Excited Component</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Pacing -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Pacing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Behaviour of walking with regular slow or fast paces, especially without a particular destination</rdfs:comment>
        <rdfs:label xml:lang="en">Pacing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Pain -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Pain">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicalProblem"/>
        <rdfs:comment xml:lang="en">It refers to the highly unpleasant physical sensation caused by illness or injury.</rdfs:comment>
        <rdfs:label xml:lang="en">Pain</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PassiveMusicTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PassiveMusicTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MusicTherapy"/>
        <ncit:synonym xml:lang="en">Receptive Music Therapy</ncit:synonym>
        <rdfs:comment xml:lang="en">It encompasses techniques that allow the participant to listen to music as opposed to being an active contributor. The music used may be live or recorded and of any genre.</rdfs:comment>
        <rdfs:label xml:lang="en">Passive Music Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PastExperience -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PastExperience">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Characteristic"/>
        <rdfs:comment xml:lang="en">It refers to the past events, knowledge, and feelings that make up one&apos;s life.</rdfs:comment>
        <rdfs:label xml:lang="en">Past Experience</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Person -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Person">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/providesBackgroundInformation"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BackgroundOfPersonWithDementia"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to people involved in the residential long term care, such as people living with dementia in the long term care facilities, and people delivering care to these demented people.</rdfs:comment>
        <rdfs:label xml:lang="en">Person</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PersonCentredCare -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PersonCentredCare">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementApproach"/>
        <rdfs:comment xml:lang="en">Person-centred care is involves tailoring a person&apos;s care to their interests, abilities, history and personality. It helps to ensure people with dementia can take part in the things they enjoy.</rdfs:comment>
        <rdfs:label xml:lang="en">Person Centred Care</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PersonCentredCareTraining -->

