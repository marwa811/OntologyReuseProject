        <ncit:synonym xml:lang="en">Ongoing Assessment</ncit:synonym>
        <rdfs:comment xml:lang="en">After the administration of the intervention, there should be ongoing monitoring of symptoms to determine the success of changes in care.</rdfs:comment>
        <rdfs:label xml:lang="en">Evaluation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EvaluationOfInteractionOfAgitationSymptomWithEnvironment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EvaluationOfInteractionOfAgitationSymptomWithEnvironment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ComprehensiveInitalAssessment"/>
        <rdfs:comment xml:lang="en">It refers to the activity of evaluating of the interaction of agitation symptom with the environment (antecedents, consequences).</rdfs:comment>
        <rdfs:label xml:lang="en">Evaluation of Interaction of Agitation Symptom with Environment</rdfs:label>
    </owl:Class>
    


