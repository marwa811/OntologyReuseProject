        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicalEnvironmentFactor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isAddressedToReduceAgitationBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ChangingAuditoryStimuli"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Too much noise can cause agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Noise</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NonhumanContactIntervention -->

