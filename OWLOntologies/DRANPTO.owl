        <rdfs:comment xml:lang="en">Using a curse against</rdfs:comment>
        <rdfs:label xml:lang="en">Cursing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/DICEApproach -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/DICEApproach">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementApproach"/>
        <rdfs:comment xml:lang="en">Describe, Investigate, Create, Evaluate (DICE) approach is an adaption of the reasoning process used by health care professionals to spot behavioural triggers and to use behavioural management skills.</rdfs:comment>
        <rdfs:label xml:lang="en">DICE Approach</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Dancing -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Dancing">
