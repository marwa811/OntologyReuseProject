        <rdfs:comment xml:lang="en">It refers to the activity of move rhythmically to music, typically following a set sequence of steps.</rdfs:comment>
        <rdfs:label xml:lang="en">Dancing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/DecreasedFrontalLobeMetabolism -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/DecreasedFrontalLobeMetabolism">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CerebralImpairment"/>
