        <rdfs:comment xml:lang="en">It refers to the actual ethnic behaviours that individuals practice, along with their attitudes toward their ethnic group.</rdfs:comment>
        <rdfs:label xml:lang="en">Ethnicity Role Identity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Evaluation -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Evaluation">
