        <rdfs:label xml:lang="en">Electronic Monitoring Intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EmotionalDistress -->

