                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PersonWithDementia"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isAssessedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationAssessmentTool"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isCausedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationCausativeFactor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isTreatedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Intervention"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Agitated behaviour is defined as any inappropriate physical or verbal, aggressive or non-aggressive, act that is not an obvious expression of need or disorientation (Cohen-Mansfield, Libin &amp; Marx 2007).</rdfs:comment>
        <rdfs:label xml:lang="en">Agitation in Dementia</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementActivity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementActivity">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isConductedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/HealthCareProfessional"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the activities conducted for managing agitation in dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Agitation Management Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementApproach -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementApproach">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isAppliedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/HealthCareProfessional"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the approach that health care professional used to manage agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Agitation Management Approach</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AlcoholEffect -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AlcoholEffect">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrapersonalCausativeFactor"/>
        <rdfs:comment xml:lang="en">It refers to the effect caused by drinking alcohol.</rdfs:comment>
        <rdfs:label xml:lang="en">Alcohol Effect</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AllowingEnoughTimeForInteraction -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AllowingEnoughTimeForInteraction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communication technique that health care professionals should be flexible and always allow plenty of time for a response from a person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Allowing Enough Time For Interaction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AlteredActivityInNeurotransmitterSystem -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AlteredActivityInNeurotransmitterSystem">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CerebralImpairment"/>
        <rdfs:comment xml:lang="en">It refers to the changed activity in the neurotransmitter system.</rdfs:comment>
        <rdfs:label xml:lang="en">Altered Activity in Neurotransmitter System</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Anesthesia -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Anesthesia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicalIllness"/>
        <ncit:synonym xml:lang="en">Anaesthesia</ncit:synonym>
        <rdfs:comment xml:lang="en">Anesthesia or anaesthesia (from Greek &quot;without sensation&quot;) is a state of controlled, temporary loss of sensation or awareness that is induced for medical purposes. It may include analgesia (relief from or prevention of pain), paralysis (muscle relaxation), amnesia (loss of memory), or unconsciousness.</rdfs:comment>
        <rdfs:label xml:lang="en">Anesthesia</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Anger -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Anger">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EmotionalDistress"/>
        <ncit:synonym xml:lang="en">Intermittent Explosive Disorder</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the unwarranted anger which is typified by hostility, impulsivity, and recurrent aggressive outbursts.</rdfs:comment>
        <rdfs:label xml:lang="en">Anger</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AnimalAssistedTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AnimalAssistedTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NonhumanContactIntervention"/>
        <ncit:abbreviation xml:lang="en">AAT</ncit:abbreviation>
        <rdfs:label xml:lang="en">Animal Assisted Therapy</rdfs:label>
        <skos:definition xml:lang="en">Animal assisted therapy is a psychotherapy treatment based upon building a bond with an animal through interaction and play. Define by Sunrise medical 2018 https://www.sunrisemedical.com.au/blog/animal-assisted-therapy-in-australia</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Anxiety -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Anxiety">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PsychiatricSyndrome"/>
        <rdfs:comment xml:lang="en">Anxiety the person may become nervous, fidgety, shaky, or frightened because of exaggerated fears that often have little basis in reality.</rdfs:comment>
        <rdfs:label xml:lang="en">Anxiety</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ApologizingForDiscomfort -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ApologizingForDiscomfort">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communication technique that the health care professional should apologize to a person with dementia if he or she did something make the person uncomfortable. Even if the health care professional had no intention of harming or angering this person - or if the health care professional don&apos;t even know why this person was not happy with him/her - the health care professional can still mend the relationship by apologizing.</rdfs:comment>
        <rdfs:label xml:lang="en">Apologizing For Discomfort</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ApproachingPersonSlowlyFromFront -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ApproachingPersonSlowlyFromFront">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communicaiton technique that health care professionals should approach the person slowly from the front and give him or her time to get used to the presence of health care professionals.</rdfs:comment>
        <rdfs:label xml:lang="en">Approaching Person  Slowly From the Front</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AppsIntervention -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AppsIntervention">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TechnologyAssistedIntervention"/>
        <rdfs:comment xml:lang="en">It refers to the treatment that involves of using apps. The apps are usually selected by the health care professionals according to the  preferences and hobbies of a person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Apps Intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Arguing -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Arguing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/VerballyAggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">It refers to the expressing diverging or opposite views, typically in a heated or angry way.</rdfs:comment>
        <rdfs:label xml:lang="en">Arguing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromaAcupressure -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromaAcupressure">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MultisensoryStimulation"/>
        <rdfs:comment xml:lang="en">The Baihui, Fengchi, Shenmen, Neiguan, and Sanyinjiao, acupuncture points have been used in the aroma-acupressure therapy to treat agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Aroma Acupressure</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Aromatherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Aromatherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryTherapy"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/reducesAgitationCausedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Smell"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Aromatherapy: use of pleasant aromatic substances (e.g., Melissa oil) for their calming effect</rdfs:comment>
        <rdfs:label xml:lang="en">Aromatherapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromatherapyUsingChamomileOil -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromatherapyUsingChamomileOil">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Aromatherapy"/>
        <rdfs:comment xml:lang="en">It refers to the aromatherapy using chamomile oil.</rdfs:comment>
        <rdfs:label xml:lang="en">Aromatherapy Using Chamomile Oil</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromatherapyUsingLavenderOil -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromatherapyUsingLavenderOil">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Aromatherapy"/>
        <rdfs:comment xml:lang="en">Aromatherapy in terms of the use of essential oils with lavender (Lavandula angustifolia)</rdfs:comment>
        <rdfs:label xml:lang="en">Aromatherapy Using Lavender Oil</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromatherapyUsingMelissaOil -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromatherapyUsingMelissaOil">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Aromatherapy"/>
        <ncit:synonym xml:lang="en">Aromatherapy Using Lemon Balm</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to aromatherapy using melissa oil. Melissa oil also known as Lemon Balm</rdfs:comment>
        <rdfs:label xml:lang="en">Aromatherapy Using Melissa Oil</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromatherapyUsingSunflowerOil -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromatherapyUsingSunflowerOil">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Aromatherapy"/>
        <rdfs:comment xml:lang="en">It refers to aromatherapy using sunflower oil.</rdfs:comment>
        <rdfs:label xml:lang="en">Aromatherapy Using Sunflower Oil</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromatherapyUsingVanillaOil -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AromatherapyUsingVanillaOil">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Aromatherapy"/>
        <rdfs:comment xml:lang="en">It refers to aromatherapy using vanilla oil.</rdfs:comment>
        <rdfs:label xml:lang="en">Aromatherapy Using Vanilla Oil</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ArrangingFlower -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ArrangingFlower">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Ergotherapy"/>
        <rdfs:comment xml:lang="en">It refers to the organized activity of arranging flowers.</rdfs:comment>
        <rdfs:label xml:lang="en">Arranging Flower</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ArtCognitiveActivity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ArtCognitiveActivity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RecreationalActivity"/>
        <ncit:synonym xml:lang="en">Art Therapy</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the activities that involves the use of creative techniques such as drawing, painting, and coloring to help people with dementia to express themselvies artistically and enhance cogintive function.</rdfs:comment>
        <rdfs:label xml:lang="en">Art Cognitive Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Arthritis -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Arthritis">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicalProblem"/>
        <ncit:synonym xml:lang="en">Joint Inflammation</ncit:synonym>
        <rdfs:comment xml:lang="en">Inflammation of one or more joints, causing pain and stiffness that can worsen with age.</rdfs:comment>
        <rdfs:label xml:lang="en">Arthritis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AskingPermission -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AskingPermission">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communication technique that health care professionals should always ask permission of the person with dementia before starting to conduct care activities.</rdfs:comment>
        <rdfs:label xml:lang="en">Asking Permission</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AskingSimpleQuestion -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AskingSimpleQuestion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communication technique that if the person seems frustrated and the health care professional don’t know what he or her wants, try to ask simple questions that can be answered with yes or no or one-word answers.</rdfs:comment>
        <rdfs:label xml:lang="en">Asking Simple Question</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AuditoryImpairment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AuditoryImpairment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryImpairment"/>
        <ncit:synonym xml:lang="en">Hearing Impairment</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the damaged sense of hearing.</rdfs:comment>
        <rdfs:label xml:lang="en">Auditory Impairment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AvoidingArguing -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AvoidingArguing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communication technique that health care professionals should try to talk about feelings of the person with dementia rather than arguing over facts with him/her.</rdfs:comment>
        <rdfs:label xml:lang="en">Avoiding Arguing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BackgroundOfPersonWithDementia -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BackgroundOfPersonWithDementia">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isBackgroundOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PersonWithDementia"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isMatchedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PersonWithDementiaFocusedIntervention"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isProvidedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Person"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to information about the background of person with dementia. Background information is considered stable (in the short run) and affect the way in which a person with dementia is able to perceive, experience, and/or interact with his/her environment.</rdfs:comment>
        <rdfs:label xml:lang="en">Background of Person with Dementia</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BadCold -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BadCold">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicalIllness"/>
        <rdfs:comment xml:lang="en">It refers to the viral infection which affects the nose, ears, and throat.</rdfs:comment>
        <rdfs:label xml:lang="en">Bad Cold</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BarrierToImplementNonpharmacologicalIntervention -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BarrierToImplementNonpharmacologicalIntervention">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/FactorAffectingImplementationOfNonpharmacologicalInterventionToAgitation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/hinders"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NonpharmacologicalIntervention"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/FacilitatorToImplementNonpharmacologicalIntervention"/>
        <rdfs:comment xml:lang="en">It refers to the barriers to implement nonpharmacological interventions to agitation in dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Barrier To Implement Nonpharmacological Intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BathEnvironmentManagement -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BathEnvironmentManagement">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InternalEnvironmentModification"/>
        <rdfs:comment xml:lang="en">It refers to the modification to the bath environment of people with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Bath Enviroment Management</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BathWaterTemperature -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BathWaterTemperature">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Temperature"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isAddressedToReduceAgitationBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/HavingComfortableBathWaterTemperature"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Inappropriate bath or shower water temperature can cause agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Bath Water Temperature</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BehaviourTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BehaviourTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualTherapyForAgitation"/>
        <ncit:synonym xml:lang="en">Behavioural Therapy</ncit:synonym>
        <rdfs:comment xml:lang="en">Behaviour therapy is to alter his/her behaviour through using differential reinforcement, cognitive (reality-orientation), and stimulus control (i.e. taught to associate stop sign with stopping and walking away, and beeper signal associated with toileting demand).</rdfs:comment>
        <rdfs:label xml:lang="en">Behaviour Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BehaviouralApproach -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BehaviouralApproach">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementApproach"/>
        <rdfs:comment xml:lang="en">Behavioural approaches for managing agitation: a systematic &quot;ABC&quot; (Antecedents -&gt; Behaviour -&gt; Consequences) approach to implementing a behavioural plan helps individualize treatment and monitor improvement. This entails identifying specific Antecedents of target problem Behaviour and their Consequences and devising specific strategies to address these.</rdfs:comment>
        <rdfs:label xml:lang="en">Behavioural Approach</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BehaviouralPathologyInAlzheimersDiseaseRatingScale -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BehaviouralPathologyInAlzheimersDiseaseRatingScale">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationAssessmentTool"/>
        <ncit:abbreviation xml:lang="en">BEHAVE-AD</ncit:abbreviation>
        <rdfs:comment xml:lang="en">The Behavioral Pathology in Alzheimer&apos;s Disease Rating Scale (BEHAVE-AD) is a screening instrument for the evaluation of behavioral disturbances in Alzheimer&apos;s patients.</rdfs:comment>
        <rdfs:label xml:lang="en">Behavioural Pathology In Alzheimer&apos;s Disease Rating Scale</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BeingPositive -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BeingPositive">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communication technique that health care professionals should take a positive perspective when caring for people with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Being Positive</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BeingReadTo -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BeingReadTo">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RecreationalActivity"/>
        <rdfs:comment xml:lang="en">It refers to the activity that someone is reading (some piece of writing) aloud to a person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Being Read To</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Belief -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Belief">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PsychologicalHistory"/>
        <rdfs:comment xml:lang="en">It refers to the acceptance that something exists or is true, especially one without proof.</rdfs:comment>
        <rdfs:label xml:lang="en">Belief</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BestTimeOfDayAssessment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BestTimeOfDayAssessment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ComprehensiveInitalAssessment"/>
        <rdfs:comment xml:lang="en">The assessment to identify the resident&apos;s best time of the day.</rdfs:comment>
        <rdfs:label xml:lang="en">Best Time Of The Day Assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Birdwatching -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Birdwatching">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RecreationalActivity"/>
        <rdfs:comment xml:lang="en">It refers to the activity of observing of birds. It can be done with the naked eye, through a visual enhancement device like binoculars and telescopes, by listening for bird sounds, or by watching public webcams.</rdfs:comment>
        <rdfs:label xml:lang="en">Birdwatching</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Biting -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Biting">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyAggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Behaviour of biting something or somebody</rdfs:comment>
        <rdfs:label xml:lang="en">Biting</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BladderInfection -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BladderInfection">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/UrinaryTractInfection"/>
        <rdfs:comment xml:lang="en">It is an infection in the bladder of the urinary system.</rdfs:comment>
        <rdfs:label xml:lang="en">Bladder Infection</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BoardGameActivity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BoardGameActivity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RecreationalActivity"/>
        <rdfs:comment xml:lang="en">It refers to the activity of playing a board game that is a game involves the movement of counters or other objects round a board.</rdfs:comment>
        <rdfs:label xml:lang="en">Board Game</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Boredom -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Boredom">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrapersonalCausativeFactor"/>
        <rdfs:comment xml:lang="en">The state of feeling disinterested in one&apos;s surroundings, having nothing to do or feeling that life is dull.</rdfs:comment>
        <rdfs:label xml:lang="en">Boredom</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BriefAgitationRatingScale -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BriefAgitationRatingScale">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationAssessmentTool"/>
        <ncit:synonym xml:lang="en">BARS</ncit:synonym>
        <rdfs:comment xml:lang="en">The 10-item BARS represents a brief mechanism to assess the presence and severity of physically aggressive, physically non-aggressive, and verbally agitated behaviors in elderly nursing home residents.</rdfs:comment>
        <rdfs:label xml:lang="en">Brief Agitation Rating Scale</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BriefPsychosocialTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BriefPsychosocialTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualTherapyForAgitation"/>
        <ncit:abbreviation xml:lang="en">BPST</ncit:abbreviation>
        <rdfs:comment xml:lang="en">The Brief Psychosocial Therapy intervention is framed around a daily social interaction with a care assistant, which lasts up to 30 min. The interaction is personalized for the individual and takes place with supervision from a therapist.</rdfs:comment>
        <rdfs:label xml:lang="en">Brief Psychosocial Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BrightLightTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BrightLightTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LightTherapy"/>
        <rdfs:comment xml:lang="en">The bright light therapy, which is based on the premise that exposure to direct bright light produces a calming effect on the agitated patients, was shown efficacious in reducing agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Bright Light Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Bronchitis -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Bronchitis">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicalIllness"/>
        <rdfs:comment xml:lang="en">Bronchitis is the inflammation of the larger airways in lungs, which causes an ongoing cough.</rdfs:comment>
        <rdfs:label xml:lang="en">Bronchitis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BuchananAgitationScale -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BuchananAgitationScale">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationAssessmentTool"/>
        <rdfs:comment xml:lang="en">The Buchanan Agitation Scale was published in 2000 in research paper &apos;The manifestation and nursing management of agitation in institutionalised residents with dementia&apos;. This 10-point rating scale incorporates ratings and their descriptors to use when measuring agitation in the demented elderly. A score of 10 represents the most agitated behaviour, a score of 1 the least agitated behaviour, with graded levels identified and described between the two extremes.</rdfs:comment>
        <rdfs:label xml:lang="en">Buchanan Agitation Scale</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BuildingBlockActivity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BuildingBlockActivity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RecreationalActivity"/>
        <rdfs:comment xml:lang="en">It refers to the activity of playing wooden or plastic toy bricks.</rdfs:comment>
        <rdfs:label xml:lang="en">Building Block</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BurnoutOfHealthCareProfessional -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BurnoutOfHealthCareProfessional">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EffectOnPhysicalHealthOfHealthCareProfessional"/>
        <rdfs:comment xml:lang="en">It refers to the concept that the agitated behaviour of people with dementia can cause the burnout of health care professionals.</rdfs:comment>
        <rdfs:label xml:lang="en">Burnout Of Health Care Professional</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CardGame -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CardGame">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RecreationalActivity"/>
        <rdfs:comment xml:lang="en">It refers to the activity of playing cards.</rdfs:comment>
        <rdfs:label xml:lang="en">Card Game</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CardiovascularDisease -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CardiovascularDisease">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicalIllness"/>
        <ncit:abbreviation xml:lang="en">CVD</ncit:abbreviation>
        <rdfs:comment xml:lang="en">Cardiovascular disease (CVD) is a collective term for diseases of the heart and blood vessels. The term commonly includes diseases such as coronary heart disease, heart failure, cardiomyopathy, congenital heart disease, peripheral vascular disease and stroke.</rdfs:comment>
        <rdfs:label xml:lang="en">Cardiovascular Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CareDeliveryTraining -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CareDeliveryTraining">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EducationInterventionToHealthCareProfessional"/>
