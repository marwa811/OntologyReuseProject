        <rdfs:comment xml:lang="en">Behaviour of destroying properties</rdfs:comment>
        <rdfs:label xml:lang="en">Destroying Property</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/DifferentialReinforcement -->

