        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EffectOfAgitationOnPersonWithDementia"/>
        <ncit:synonym xml:lang="en">Lower Quality Of Life</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the concept that the agitation has an effect on the individual&apos;s wellbeing and the ability to enjoy life.</rdfs:comment>
        <rdfs:label xml:lang="en">Poor Quality Of Life</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PoorStaffCooperation -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PoorStaffCooperation">
