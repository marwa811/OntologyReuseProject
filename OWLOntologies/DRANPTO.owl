        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyAggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Causing pain or injury to oneself</rdfs:comment>
        <rdfs:label xml:lang="en">Hurting Self</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/HurtingSomeone -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/HurtingSomeone">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyAggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Causing pain or injury to someone</rdfs:comment>
        <rdfs:label xml:lang="en">Hurting Someone</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hyperthermia -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hyperthermia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MetabolicDisorder"/>
        <rdfs:comment xml:lang="en">It refers to a group of heat-related conditions characterized by an abnormally high body temperature.</rdfs:comment>
        <rdfs:label xml:lang="en">Hyperthermia</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hyperthyroidism -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hyperthyroidism">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MetabolicDisorder"/>
        <ncit:synonym xml:lang="en">Overactive Thyroid</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the overproduction of a hormone by the butterfly-shaped gland in the neck (thyroid).</rdfs:comment>
        <rdfs:label xml:lang="en">Hyperthyroidism</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hypoglycemia -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hypoglycemia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MetabolicDisorder"/>
        <ncit:synonym xml:lang="en">Low Blood Sugar</ncit:synonym>
        <rdfs:comment xml:lang="en">Hypoglycemia, also known as low blood sugar, is when blood sugar decreases to below normal levels. This may result in a variety of symptoms including clumsiness, trouble talking, confusion, loss of consciousness, seizures or death. A feeling of hunger, sweating, shakiness and weakness may also be present.</rdfs:comment>
        <rdfs:label xml:lang="en">Hypoglycemia</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hypothermia -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hypothermia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MetabolicDisorder"/>
        <rdfs:comment xml:lang="en">Hypothermia is a medical emergency that occurs when your body loses heat faster than it can produce heat, causing a dangerously low body temperature. Normal body temperature is around 98.6 F (37 C). Hypothermia (hi-poe-THUR-me-uh) occurs as your body temperature falls below 95 F (35 C). The symptom is confusion.</rdfs:comment>
        <rdfs:label xml:lang="en">Hypothermia</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hypothyroidism -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hypothyroidism">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MetabolicDisorder"/>
        <ncit:synonym xml:lang="en">Underactive Thyroid</ncit:synonym>
        <rdfs:comment xml:lang="en">Hypothyroidism is a health condition characterised by unusually low hormone production. It is when your thyroid gland becomes underactive and cannot produce enough hormones to regulate your metabolism.</rdfs:comment>
        <rdfs:label xml:lang="en">Hypothyroidism</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hypoxia -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Hypoxia">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicalIllness"/>
        <rdfs:comment xml:lang="en">An absence of enough oxygen in the tissues to sustain bodily functions.</rdfs:comment>
        <rdfs:label xml:lang="en">Hypoxia</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ImpairmentInAbilityToPerformDailyLivingActivity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ImpairmentInAbilityToPerformDailyLivingActivity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PoorQualityOfLife"/>
        <rdfs:comment xml:lang="en">It refers to the concept that agitation could cause impairment in the ability to perform the activities of daily living to person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Impairment In Ability To Perform Daily Living Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ImpairmentInInterpersonalRelationship -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ImpairmentInInterpersonalRelationship">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PoorQualityOfLife"/>
        <rdfs:comment xml:lang="en">It refers to the concept that the agitation could cause the impairment in interpersonal relationships to person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Impairment In Interpersonal Relationship</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ImpairmentInSocialFunctioning -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ImpairmentInSocialFunctioning">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PoorQualityOfLife"/>
        <rdfs:comment xml:lang="en">It refers to the concept that agitation could cause the impairment of person with dementia in the social functioning.</rdfs:comment>
        <rdfs:label xml:lang="en">Impairment In Social Functioning</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ImportantRelationship -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ImportantRelationship">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Characteristic"/>
        <rdfs:comment xml:lang="en">It refers to the relationship that is important for the person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Important Relationship</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ImpulsivityAssessment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ImpulsivityAssessment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ComprehensiveInitalAssessment"/>
        <rdfs:comment xml:lang="en">It refers to the activity of assessing the impulsivity in people with dementia. Impulsivity has been found to reflect lack of  forward planning and sustained attention rather than risk or sensation seeking.</rdfs:comment>
        <rdfs:label xml:lang="en">Impulsivity Assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriateDressing -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriateDressing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Behaviour of wearing clothes that is not suitable or proper in the circumstances.</rdfs:comment>
        <rdfs:label xml:lang="en">Inappropriate Dressing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriateNameCalling -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriateNameCalling">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/HealthCareProfessionalVerbalBehaviour"/>
        <ncit:synonym xml:lang="en">Unappropriate Name Calling</ncit:synonym>
        <rdfs:comment xml:lang="en">Calling the person inappropriately can cause agitation. For example, some person with dementia don&apos;t like to be called by their first name.</rdfs:comment>
        <rdfs:label xml:lang="en">Inappropriate Name Calling</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriateRoutine -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriateRoutine">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SocialEnvironmentFactor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isAddressedToReduceAgitationBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ReschedulingRoutine"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <ncit:synonym xml:lang="en">Unappropriate Routine</ncit:synonym>
        <rdfs:comment xml:lang="en">The routine that is not suitable for the person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Inappropriate Routine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriateUndressing -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriateUndressing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyNonaggressiveBehaviour"/>
        <ncit:synonym xml:lang="en">Inappropriate Disrobing</ncit:synonym>
        <rdfs:comment xml:lang="en">Behaviour of taking off clothes in the inappropriate circumstances</rdfs:comment>
        <rdfs:label xml:lang="en">Inappropriate Undressing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriatelyHandlingObject -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InappropriatelyHandlingObject">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Inappropriate handling of objects, for example, pulling at tubing (Foley catheter), twisting phone cord, dumping water jug, pulling tissues out of box and throwing them on the floor</rdfs:comment>
        <rdfs:label xml:lang="en">Inappropriate Handling Object</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IncreasedNeurofibrillaryTangleBurdenInFrontalLobe -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IncreasedNeurofibrillaryTangleBurdenInFrontalLobe">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CerebralImpairment"/>
        <rdfs:comment xml:lang="en">Neurofibrillary tangle burden in the frontal lobe of person in dementia is increased.</rdfs:comment>
        <rdfs:label xml:lang="en">Increased Neurofibrillary Tangle Burden in Frontal Lobe</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualPsychologicalTreatment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualPsychologicalTreatment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualTherapyForAgitation"/>
        <rdfs:comment xml:lang="en">Individual psychological treatment can help people change their thinking patterns and improve their coping skills.</rdfs:comment>
        <rdfs:label xml:lang="en">Individual Psychological Treatment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualTherapyForAgitation -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualTherapyForAgitation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PersonWithDementiaFocusedIntervention"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/composes"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MulticomponentInterventionForAgitation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the individual therapy used for managing agitation in dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Individual Therapy For Agitation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndoorGardening -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndoorGardening">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Ergotherapy"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/reducesAgitationCausedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SleepDisorder"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the organized activity of gardening in the indoor environment. For example, indoor containerized water gardening of edible dropwort and bean sprout .</rdfs:comment>
        <rdfs:label xml:lang="en">Indoor Gardening</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InfectiousDisease -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InfectiousDisease">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicalIllness"/>
        <rdfs:comment xml:lang="en">It refers to the disease that can be passed from person to person.</rdfs:comment>
        <rdfs:label xml:lang="en">Infectious Disease</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InjuryOfHealthCareProfessional -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InjuryOfHealthCareProfessional">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EffectOnPhysicalHealthOfHealthCareProfessional"/>
        <rdfs:comment xml:lang="en">It refers to the concept that the agitated behaviour of people with dementia can cause physical injury of health care professionals.</rdfs:comment>
        <rdfs:label xml:lang="en">Injury Of Health Care Professional</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Interest -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Interest">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Characteristic"/>
        <rdfs:comment xml:lang="en">It refers to the activity or subject which one enjoys doing or studying.</rdfs:comment>
        <rdfs:label xml:lang="en">Interest</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InternalEnvironmentModification -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InternalEnvironmentModification">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicalEnvironmentalIntervention"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/treats"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Restlessness"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <ncit:synonym>Indoor environment modification</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the intervention of modifying the indoor environment of people with dementia.</rdfs:comment>
        <rdfs:label>Internal Environment Modification</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InternetVideoChatFamilyConversation -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InternetVideoChatFamilyConversation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RealHumanSocialContactIntervention"/>
        <rdfs:comment xml:lang="en">Internet video chat (Skype) family conversations as a treatment may have a positive effect in reducing agitation in residents with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Internet Video Chat Family Conversation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InterpersonalCausativeFactor -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InterpersonalCausativeFactor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationCausativeFactor"/>
        <rdfs:comment xml:lang="en">The communication between person with dementia and other people can cause agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Interpersonal Causative Factor</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InterpretingConfusedVerbalCommunication -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InterpretingConfusedVerbalCommunication">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communication technique of interpreting confused verbal communication.</rdfs:comment>
        <rdfs:label xml:lang="en">Interpreting Confused Verbal Communication</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InterpretingNonverbalCommunication -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InterpretingNonverbalCommunication">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communication technique of interpreting nonverbal communication, such as gestures, facial expressions, eye contact, tone of voice, body language, and other ways people using for communicating without using language.</rdfs:comment>
        <rdfs:label xml:lang="en">Interpreting Nonverbal Communication</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Intervention -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Intervention">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementActivity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/treats"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationInDementia"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the actual treatments and actions that are performed to improve agitation symptom of a person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrapersonalCausativeFactor -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrapersonalCausativeFactor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationCausativeFactor"/>
        <rdfs:comment xml:lang="en">It refers to the things that can cause agitation exclusively related to himself/herself.</rdfs:comment>
        <rdfs:label xml:lang="en">Intrapersonal Causative Factor</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrusionIntoPersonalPrivacy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrusionIntoPersonalPrivacy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ResidentBehaviour"/>
        <rdfs:comment xml:lang="en">Intrusion into the privacy of person with dementia can cause agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Intrusion Into Personal Privacy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrusionIntoPersonalSpace -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrusionIntoPersonalSpace">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ResidentBehaviour"/>
        <rdfs:comment xml:lang="en">Intrusion into the resident’s own personal space, such as bathing and toileting space, can cause agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Intrusion Into Personal Space</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Irritability -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Irritability">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EmotionalDistress"/>
        <rdfs:comment xml:lang="en">Feelings of frustration or anger, often over seemingly small matters.</rdfs:comment>
        <rdfs:label xml:lang="en">Irritability</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/JudgmentAbilityAssessment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/JudgmentAbilityAssessment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ComprehensiveInitalAssessment"/>
        <rdfs:comment xml:lang="en">It refers to the activity of assessing people&apos;s capacity to make one&apos;s own decisions.</rdfs:comment>
        <rdfs:label xml:lang="en">Judgment Ability Assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/KeepingCalm -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/KeepingCalm">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">Keep calm means to not get emotionally invested in situations, events, or people that one has absolutely no control over. Showing anger can make the person’s agitation worse. If health care professionals are about to lose the temper, try to “counting to ten.”</rdfs:comment>
        <rdfs:label xml:lang="en">Keeping Calm</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Kicking -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Kicking">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicallyAggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Behaviour of kicking someone or something</rdfs:comment>
        <rdfs:label xml:lang="en">Kicking</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LackOfAppropriateEngagingActivity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LackOfAppropriateEngagingActivity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SocialEnvironmentFactor"/>
        <rdfs:comment xml:lang="en">Lack of appropriate engaging activities for person with dementia can cause agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Lack Of Appropriate Engaging Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LackOfNaturalMaterial -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LackOfNaturalMaterial">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicalEnvironmentFactor"/>
        <rdfs:comment xml:lang="en">The lack of nature and natural materials, such as flowers, a view or pictures of nature, in the environment can cause manifestation of agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Lack Of Natural Material</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LevelOfPhysicalActivity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LevelOfPhysicalActivity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicalHealth"/>
        <rdfs:comment xml:lang="en">It refers to the level of a person&apos;s daily physical activity and is used to estimate a person&apos;s total energy expenditure.</rdfs:comment>
        <rdfs:label xml:lang="en">Level of Physical Activity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Light -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Light">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PhysicalEnvironmentFactor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isAddressedToReduceAgitationBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ChangingVisualStimuli"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the inappropriate light which can cause agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Light</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LightTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LightTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryTherapy"/>
        <rdfs:comment xml:lang="en">Exposure to certain type of light can help people with dementia to reduce agitation.</rdfs:comment>
        <rdfs:label xml:lang="en">Light Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LikelihoodOfInstitutionalisation -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LikelihoodOfInstitutionalisation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EffectOfAgitationOnSociety"/>
        <rdfs:comment xml:lang="en">It refers to the concept that dementia-related agitation can cause the likelihood of institutionalisation.</rdfs:comment>
        <rdfs:label xml:lang="en">Likelihood Of Institutionalisation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LikelihoodOfMedicalComorbidity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LikelihoodOfMedicalComorbidity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EffectOfAgitationOnPersonWithDementia"/>
        <rdfs:comment xml:lang="en">It refers to the concept that the agitation could cause the likelihood of medical comorbidity to person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Likelihood Of Medical Comorbidity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LikelihoodOfPsychiatricComorbidity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LikelihoodOfPsychiatricComorbidity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EffectOfAgitationOnPersonWithDementia"/>
        <rdfs:comment xml:lang="en">It refers to the concept that the agitation could cause the likelihood of psychiatric comorbidity to person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Likelihood Of Psychiatric Comorbidity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ListeningClassicMusic -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ListeningClassicMusic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PassiveMusicTherapy"/>
        <rdfs:comment xml:lang="en">It requires people with dementia to listen to classic music.</rdfs:comment>
        <rdfs:label xml:lang="en">Listening Classic Music</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ListeningPreferredMusic -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ListeningPreferredMusic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PassiveMusicTherapy"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/reducesAgitationCausedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SleepDisorder"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Listening to the personal perferred music, e.g., folk songs from childhood</rdfs:comment>
        <rdfs:label xml:lang="en">Listening Preferred Music</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ListeningRelaxingMusic -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ListeningRelaxingMusic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PassiveMusicTherapy"/>
        <ncit:synonym xml:lang="en">Listening Calming Music, Listening Soothing Music</ncit:synonym>
        <rdfs:comment xml:lang="en">Relaxing music has a positive effect in reducing agitation during meals among nursing home residents with dementia. The music used in one study was “Sea Gulls … Music for Rest and Relaxation,” by Palmer in 1978. The relaxing music was composed to promote rest and relaxation by being quiet, melodical, and peaceful, without sudden changes to tempo or volume, yet with sufficient variation to avoid boredom.</rdfs:comment>
        <rdfs:label xml:lang="en">Listening Relaxing Music</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ListeningWithRespect -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ListeningWithRespect">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communication technique of listening with respect. Listening with respect isn&apos;t the same as listening for ammunition. It&apos;s listening for truth. It&apos;s giving the person time to talk. It&apos;s asking clarifying questions and briefly summarizing or restating every now and then to insure what the health care professional is hearing is the same as what the person with dementia is trying to communicate.</rdfs:comment>
        <rdfs:label xml:lang="en">Listening with Respect</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LocatingBathSupplyNearBathingArea -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LocatingBathSupplyNearBathingArea">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BathEnvironmentManagement"/>
        <rdfs:comment xml:lang="en">It refers to the location of bath supply that should be close to the bathing area.</rdfs:comment>
        <rdfs:label xml:lang="en">Locating Bath Supply Near Bathing Area</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Loneliness -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Loneliness">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrapersonalCausativeFactor"/>
        <rdfs:comment xml:lang="en">Feeling sad and unhappy about being socially isolated.</rdfs:comment>
        <rdfs:label xml:lang="en">Loneliness</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MaintainingEyeContact -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MaintainingEyeContact">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/CommunicationTechnique"/>
        <rdfs:comment xml:lang="en">It refers to the communication technique that the health care professional should maintain appropriate eye contact during the delivery of care to a person with dementia. To maintain appropriate eye contact without starring, a person should maintain eye contact for 50 percent of the time while speaking and 70% of the time while listening.</rdfs:comment>
        <rdfs:label xml:lang="en">Maintaining Eye Contact</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MakingBizarreNoise -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MakingBizarreNoise">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/VerballyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">Making very strange or unusual noise</rdfs:comment>
        <rdfs:label xml:lang="en">Making Bizarre Noise</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MakingPuzzle -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MakingPuzzle">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RecreationalActivity"/>
        <rdfs:comment xml:lang="en">It refers to the activity of making puzzle.</rdfs:comment>
        <rdfs:label xml:lang="en">Making Puzzle</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Malnutrition -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Malnutrition">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MetabolicDisorder"/>
        <ncit:synonym xml:lang="en">Poor Nutrition</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the lack of sufficient nutrients in the body.</rdfs:comment>
        <rdfs:label xml:lang="en">Malnutrition</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ManagementSupport -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ManagementSupport">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/FacilitatorToImplementNonpharmacologicalIntervention"/>
        <rdfs:comment xml:lang="en">It refers to the support from management of the long-term care facilities, including managers at all levels.</rdfs:comment>
        <rdfs:label xml:lang="en">Management Support</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MassageTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MassageTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryTherapy"/>
        <rdfs:comment xml:lang="en">It refers to the use of massage as a non-pharmacological approach to managing agitation in older people with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Massage Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicalIllness -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicalIllness">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrapersonalCausativeFactor"/>
        <rdfs:comment xml:lang="en">It refers to the disease which can cause agitation in dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Medical Illness</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationChange -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationChange">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationEffect"/>
        <rdfs:comment xml:lang="en">It refers to the changes of medication prescribed for the person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Medication Change</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationEffect -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationEffect">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IntrapersonalCausativeFactor"/>
        <rdfs:comment xml:lang="en">It refers to the effect caused by medication or drug.</rdfs:comment>
        <rdfs:label xml:lang="en">Medication Effect</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationSideEffect -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationSideEffect">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationEffect"/>
        <ncit:synonym xml:lang="en">Medication Adverse Effect, Medication Adverse Reaction</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to any unwanted or unexpected effects of a medicine.</rdfs:comment>
        <rdfs:label xml:lang="en">Medication Side Effect</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationToxicity -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationToxicity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicationEffect"/>
        <ncit:synonym xml:lang="en">Drug Toxicity</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the health problems that occur as a result of the over-ingestion of a medication - having too much of a drug in a person&apos;s system at once.</rdfs:comment>
        <rdfs:label xml:lang="en">Medication Toxicity</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MemoryBoxIntervention -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MemoryBoxIntervention">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/TechnologyAssistedIntervention"/>
        <rdfs:comment xml:lang="en">It refers to the treatment that involves of using Memory Box which is a personal computer to offer digitalized music, photographs, movies and messages chosen or made by family members to reflect residents’ backgrounds and preferences.</rdfs:comment>
        <rdfs:label xml:lang="en">Memory Box Intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Meningitis -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Meningitis">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/InfectiousDisease"/>
        <rdfs:comment xml:lang="en">Meningitis is an inflammation of the meninges. The meninges are the membranes surrounding the brain and spinal cord.</rdfs:comment>
        <rdfs:label xml:lang="en">Meningitis</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MentalHealth -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MentalHealth">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/HealthCondition"/>
        <rdfs:comment xml:lang="en">It refers to the level of psychological well-being.</rdfs:comment>
        <rdfs:label xml:lang="en">Mental Health</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MetabolicDisorder -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MetabolicDisorder">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MedicalIllness"/>
        <rdfs:comment xml:lang="en">A metabolic disorder occurs when the metabolism process fails and causes the body to have either too much or too little of the essential substances needed to stay healthy.</rdfs:comment>
        <rdfs:label xml:lang="en">Metabolic Disorder</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MiniMentalStateExamination -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MiniMentalStateExamination">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationAssessmentTool"/>
        <ncit:abbreviation xml:lang="en">MMSE</ncit:abbreviation>
        <rdfs:comment xml:lang="en">The MMSE was designed as a screening test for the purpose of evaluating cognitive impairment in older adults. The maximum MMSE score is 30 points. A score of 20 to 24 suggests mild dementia, 13 to 20 suggests moderate dementia, and less than 12 indicates severe dementia. On average, the MMSE score of a person with Alzheimer&apos;s declines about two to four points each year.</rdfs:comment>
        <rdfs:label xml:lang="en">Mini Mental State Examination</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MinimizingGlare -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MinimizingGlare">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/BathEnvironmentManagement"/>
        <rdfs:comment xml:lang="en">It refers to the modification of reducing the glare in the bathroom to the lowest level.</rdfs:comment>
        <rdfs:label xml:lang="en">Minimizing Glare</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MinimizingTimeOfPersonUnclothed -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MinimizingTimeOfPersonUnclothed">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EnsuringPrivacyDuringBathing"/>
        <rdfs:comment xml:lang="en">It refers to the action of minimizing the time of the person unclothed during bathing.</rdfs:comment>
        <rdfs:label xml:lang="en">Minimizing Time Of Person Unclothed</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Mobility -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Mobility">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/RemainingAbility"/>
        <rdfs:comment xml:lang="en">It refers to the ability to move freely and easily.</rdfs:comment>
        <rdfs:label xml:lang="en">Mobility</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MobilityImpairment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MobilityImpairment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Deficiency"/>
        <rdfs:comment xml:lang="en">It refers to the damaged ability to move physically.</rdfs:comment>
        <rdfs:label xml:lang="en">Mobility Impairment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MonotonyOfNursingHomeEnvironment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MonotonyOfNursingHomeEnvironment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EnvironmentalCausativeFactor"/>
        <rdfs:comment xml:lang="en">The lack of variety and interest in nursing home environment can cause agitation of person with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Monotony of Nursing Home Environment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MontessoriApproach -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MontessoriApproach">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementApproach"/>
        <ncit:synonym xml:lang="en">Montessori Method</ncit:synonym>
        <rdfs:comment xml:lang="en">Montessori Approach of caregiving for dementia has a similar goal of engaging the senses in order to help person with dementia rediscover the world around them. Providing the most effective care means maximizing the opportunities these individuals have to reconnect with a world they are losing access to.</rdfs:comment>
        <rdfs:label xml:lang="en">Montessori Approach</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MoodAssessment -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MoodAssessment">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/ComprehensiveInitalAssessment"/>
        <rdfs:comment xml:lang="en">It refers to the activity of assessing the mood and mood changes in people with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Mood Assessment</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MoodSwing -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MoodSwing">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EmotionalDistress"/>
        <rdfs:comment xml:lang="en">It refers to rapid change in mood.</rdfs:comment>
        <rdfs:label xml:lang="en">Mood Swing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MorningLightTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MorningLightTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/LightTherapy"/>
        <rdfs:comment xml:lang="en">It refers to the light exprosure during the morning.</rdfs:comment>
        <rdfs:label xml:lang="en">Morning Light Therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MulticomponentInterventionForAgitation -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MulticomponentInterventionForAgitation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/PersonWithDementiaFocusedIntervention"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/isComposedOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/IndividualTherapyForAgitation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the mixture and application of interventions in combination. For example, an organized program of music therapy, orientation training, art-cognitive activities and physical activities.</rdfs:comment>
        <rdfs:label xml:lang="en">Multicomponent Intervention For Agitation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MultisensoryStimulation -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MultisensoryStimulation">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryTherapy"/>
        <ncit:abbreviation xml:lang="en">MSS</ncit:abbreviation>
        <ncit:synonym>Multisensory Intervention</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the treatment of using snoezelen room or a combination of music, touch, and smell.</rdfs:comment>
        <rdfs:label xml:lang="en">Multisensory Stimulation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MusicTherapy -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MusicTherapy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SensoryTherapy"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/reducesAgitationCausedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Noise"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <ncit:abbreviation xml:lang="en">MT</ncit:abbreviation>
        <ncit:synonym xml:lang="en">Music Intervention</ncit:synonym>
        <rdfs:comment xml:lang="en">It refers to the use of music and its influence on people with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Music Therapy</rdfs:label>
        <skos:definition xml:lang="en">The American Music Therapy Association (AMTA) defines music therapy as “the clinical and evidence-based use of music intervention to accomplish individualized goals within a therapeutic relationship by a credentialed professional who has completed an approved music therapy program” (American Music Therapy Association 2006).</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MusicWithHandMassage -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MusicWithHandMassage">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/MultisensoryStimulation"/>
        <rdfs:comment xml:lang="en">It refers to the intervention of using both music and hand massage to manage agitation in dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Music with Hand Massage</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NatureSoundIntervention -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NatureSoundIntervention">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/SoundIntervention"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/reducesAgitationCausedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Noise"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It refers to the intervention of using relaxing nature sounds from the forest, river, lake, ocean and waterfall with and without birdsong.</rdfs:comment>
        <rdfs:label xml:lang="en">Nature Sound Intervention</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NeedDrivenDementiaCompromisedBehaviourModel -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NeedDrivenDementiaCompromisedBehaviourModel">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationManagementApproach"/>
        <ncit:abbreviation xml:lang="en">NDB Model</ncit:abbreviation>
        <rdfs:comment xml:lang="en">Need Driven Dementia-Compromised Behaviour Model changes the prevalent view of dementia-related behaviour as &quot;disruptive&quot; or &quot;disturbing&quot; to a perspective that conceptualizes these behaviours as potentially understandable needs that, if responded to appropriately, will reduce these behaviours and enhance quality of life.</rdfs:comment>
        <rdfs:label xml:lang="en">Need Driven Dementia Compromised Behaviour Model</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Negativism -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Negativism">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EmotionalDistress"/>
        <rdfs:comment xml:lang="en">The practice of being or tendency to be negative or sceptical in attitude while failing to offer positive suggestions or views.</rdfs:comment>
        <rdfs:label xml:lang="en">Negativism</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NeuropsychiatricInventory -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NeuropsychiatricInventory">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationAssessmentTool"/>
        <ncit:abbreviation xml:lang="en">NPI</ncit:abbreviation>
        <rdfs:comment xml:lang="en">The Neuropsychiatric Inventory (NPI) was developed by Cummings et al. to assess dementia-related behavioral symptoms. The NPI originally examined 10 sub-domains of behavioral functioning: delusions, hallucinations, agitation/aggression, dysphoria, anxiety, euphoria, apathy, disinhibition, irritability/lability, and aberrant motor activity. Two more sub-domains have been added since its development: night-time behavioral disturbances and appetite and eating abnormalities.</rdfs:comment>
        <rdfs:label xml:lang="en">Neuropsychiatric Inventory</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NeuropsychiatricInventoryNursingHomeVersion -->

    <owl:Class rdf:about="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/NeuropsychiatricInventoryNursingHomeVersion">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/AgitationAssessmentTool"/>
        <ncit:abbreviation xml:lang="en">NPI-NH</ncit:abbreviation>
        <rdfs:comment xml:lang="en">It was developed to help characterize the neuropsychiatric symptoms and psychopathology of patients with Alzheimer’s disease and other dementias when the patients are residents in extended care facilities or other care settings where information is gathered from health care professionals. The NPI-NH was derived from the Neuropsychiatric Inventory (NPI), which was originally developed for the assessment of neuropsychiatric symptoms and psychopathology in community-dwelling patients where information was obtained from family caregivers. The content of the questions of the NPI and NPI-NH are identical but have been rephrased appropriately. In addition, the caregiver distress scale of the NPI has been changed to an occupational disruptiveness scale for the NPI-NH to allow an assessment of the impact of behavioral disturbances on professional caregivers.</rdfs:comment>
        <rdfs:label xml:lang="en">Neuropsychiatric Inventory - Nursing Home Version</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/Noise -->

