        <rdfs:comment xml:lang="en">It refers to the intervention of interaction with non human, such as animals and robots.</rdfs:comment>
        <rdfs:label xml:lang="en">Nonhuman Contact Intervention</rdfs:label>
    </owl:Class>
    


