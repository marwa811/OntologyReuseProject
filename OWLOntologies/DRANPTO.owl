        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/EffectOfAgitationOnHealthCareProfessional"/>
        <rdfs:comment xml:lang="en">It refers to the effect that the daily care routine would be  disrupted by the agitated behaviour of people with dementia.</rdfs:comment>
        <rdfs:label xml:lang="en">Disruption To Care Routine</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/DisruptiveBehaviourRatingScale -->

