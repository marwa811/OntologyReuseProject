        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/zhenyuzhang/ontologies/2019/8/nonpharmacological-intervention-for-agitation-in-dementia-ontology/VerballyNonaggressiveBehaviour"/>
        <rdfs:comment xml:lang="en">It refers to the constant and not justified requests for attention</rdfs:comment>
        <rdfs:label xml:lang="en">Constant Unwarranted Requests for Attention</rdfs:label>
    </owl:Class>
    


